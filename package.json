{
  "name": "ps-redux",
  "version": "1.0.0",
  "description": "Starter kit for React and Redux Pluralsight course by Cory House",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js src/**/*.test.js src/components/**/*.test.js",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist  build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.9.8",
    "@material-ui/pickers": "^3.2.10",
    "babel-polyfill": "6.8.0",
    "bootstrap": "3.3.6",
    "bootstrap-material-datetimepicker": "^2.7.3",
    "create-react-class": "^15.6.0",
    "font-awesome": "^4.7.0",
    "jquery": "2.2.3",
    "material-ui": "^0.16.7",
    "material-ui-time-picker": "^1.3.0",
    "moment": "^2.17.1",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-input-mask": "^0.8.0",
    "react-redux": "4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "4.0.4",
    "react-tap-event-plugin": "^2.0.1",
    "react-tappable": "^1.0.0",
    "react-time-picker": "^4.0.0",
    "react-touch": "^0.4.2",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "6.26.3",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "colors": "1.1.2",
    "compression": "^1.6.1",
    "cross-env": "1.0.7",
    "css-loader": "^0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.8.5",
    "jsdom": "8.5.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "mocha": "10.1.0",
    "nock": "8.0.0",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "react-addons-test-utils": "15.0.2",
